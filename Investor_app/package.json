{
  "name": "splitflowandroid",
  "version": "1.0.1",
  "main": "index.js",
  "scripts": {
    "start": "react-native start",
    "start:metro": "react-native start",
    "android": "react-native run-android",
    "android:native": "react-native run-android",
    "adb:setup": "adb reverse tcp:3000 tcp:3000 && adb reverse tcp:8081 tcp:8081",
    "dev:android": "npm run adb:setup && react-native run-android",
    "android:bundle:release": "cd android && gradlew bundleRelease",
    "android:apk:release": "cd android && gradlew assembleRelease",
    "android:debug:win": "cd android && gradlew assembleDebug",
    "android:clean:win": "cd android && gradlew clean",
    "ios": "react-native run-ios",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e:native:build": "npx detox build -c android.emu.debug",
    "test:e2e:native": "npx detox test -c android.emu.debug --headless"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-navigation/native": "^7.1.27",
    "@react-navigation/native-stack": "^7.9.1",
    "axios": "^1.13.5",
    "prop-types": "^15.8.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "^0.75.5",
    "react-native-biometrics": "^3.0.1",
    "react-native-fs": "^2.20.0",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-safe-area-context": "^5.6.2",
    "react-native-screens": "4.16.0",
    "react-native-share": "^12.2.0",
    "react-native-vector-icons": "^10.3.0",
    "react-native-web": "^0.21.0"
  },
  "private": true,
  "devDependencies": {
    "@babel/preset-env": "^7.29.0",
    "@babel/preset-flow": "^7.27.1",
    "@babel/preset-react": "^7.28.5",
    "@react-native-community/cli": "^20.1.1",
    "@react-native/metro-config": "^0.84.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^11.0.0",
    "babel-jest": "^30.2.0",
    "jest": "^30.2.0",
    "react-test-renderer": "18.3.1"
  },
  "jest": {
    "preset": "react-native",
    "setupFilesAfterEnv": [
      "@testing-library/jest-native/extend-expect"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(react-native|@react-native|react-native-vector-icons)/)"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/android/",
      "/ios/",
      "/tests/"
    ]
  },
  "overrides": {
    "fast-xml-parser": "5.3.7",
    "braces": "^3.0.3",
    "form-data": "^4.0.0",
    "json5": "^2.2.3",
    "merge": "^2.1.1",
    "minimatch": "^10.2.2",
    "node-notifier": "^10.0.1",
    "qs": "^6.14.1",
    "tough-cookie": "^4.1.3",
    "micromatch": "^4.0.8",
    "glob": "^11.0.0",
    "test-exclude": "^8.0.0"
  }
}
